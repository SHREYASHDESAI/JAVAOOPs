package ThisKeyword;

//To change constructors

import java.math.*;
import java.io.*;
import java.util.*;


class Point2 {
    int x, y;

    Point2() {
        this(10, 10);
    }

    Point2(int x, int y) {
        this.x = x;
        this.y = y;
    }

    void print() {
        System.out.println(x + " " + y);
    }
}

public class This3
{
    public static void main(String[] args)throws IOException
    {

        Point2 p1 = new Point2(); //when this object is used it calls the parameterless constructor
        Point2 p2 = new Point2(5,15); //when this object is used it calls parameterized constructor

        p1.print();
        p2.print();


    }
}

